#include <iostream>

int main() {
  int number;
  std::cout << "Введите число: \n";
  std::cin >> number;
  int digits = 0;
  bool stop = false;
  
  while (number > 0 && !stop) {
    stop = number / 10 < 0;
    number /= 10;
    ++digits;
  }
  

  
  
  std::cout << "В введенном числе " << digits << "знаков.";
}
